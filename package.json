{
  "name": "code-blocks",
  "version": "1.0.0",
  "description": "Parse fenced code blocks from markdown with useful metadata",
  "main": "index.js",
  "dependencies": {
    "fs-extra": "^4.0.2",
    "parse-pairs": "^0.2.2",
    "remark": "^8.0.0",
    "unist-util-find-before": "^2.0.1",
    "unist-util-parents": "^1.0.0",
    "unist-util-select": "^1.5.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "prettier": "^1.7.0"
  },
  "scripts": {
    "test": "ava --verbose test/*.js",
    "watch": "ava --verbose --watch test/*.js",
    "prettier": "prettier --write **/*.js"
  },
  "prettier": {
    "bracket-spacing": false,
    "semi": false,
    "single-quote": true,
    "trailing-comma": "es5"
  },
  "keywords": [
    "code",
    "blocks",
    "markdown"
  ],
  "author": "Shawn Allen <https://github.com/shawnbot>",
  "license": "Unlicense"
}
